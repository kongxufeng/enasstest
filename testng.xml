<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="企业上云服务站测试">
	<listeners>
		<listener class-name="com.enass.utils.TestListener" />
		<listener class-name="com.enass.utils.RetryListener" />
	</listeners>

	<test name="开始评估测试">
		<classes>
			<class name="com.enass.test.assessment.Start">
				<methods>
					<include name="zhenduan" />
					<include name="save" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="未完成的评估测试">
		<classes>
			<class name="com.enass.test.assessment.Unfinished">
				<methods>
					<include name="Continue" />
					<include name="Search" />
					<include name="Delete" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="已完成的评估测试">
		<classes>
			<class name="com.enass.test.assessment.Completed">
				<methods>
					<include name="Report" />
					<include name="Detail" />
					<include name="Delete" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="本企业的评估测试">
		<classes>
			<class name="com.enass.test.assessment.Benqiye">
				<methods>
					<include name="Report" />
					<include name="Detail" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="添加视频测试">
		<classes>
			<class name="com.enass.test.neirongguanli.AddVideo">
			</class>
		</classes>
	</test>

	<test name="编辑视频测试">
		<classes>
			<class name="com.enass.test.neirongguanli.EditVideo">
			</class>
		</classes>
	</test>

	<test name="查询视频测试">
		<classes>
			<class name="com.enass.test.neirongguanli.SearchVideo">
			</class>
		</classes>
	</test>

	<test name="删除视频测试">
		<classes>
			<class name="com.enass.test.neirongguanli.DeleteVideo">
			</class>
		</classes>
	</test>

	<test name="添加解决方案测试">
		<classes>
			<class name="com.enass.test.neirongguanli.AddFile">
			</class>
		</classes>
	</test>

	<test name="编辑解决方案测试">
		<classes>
			<class name="com.enass.test.neirongguanli.EditFile">
			</class>
		</classes>
	</test>

	<test name="查询解决方案测试">
		<classes>
			<class name="com.enass.test.neirongguanli.SearchFile">
			</class>
		</classes>
	</test>

	<test name="删除解决方案测试">
		<classes>
			<class name="com.enass.test.neirongguanli.DeleteFile">
			</class>
		</classes>
	</test>

</suite>